# ios/App/fastlane/Matchfile

storage_mode("git")
type("appstore")

git_url(ENV["MATCH_GIT_URL"])
git_branch(ENV["MATCH_GIT_BRANCH"] || "master")

# IMPORTANT: Use basic auth header stored in secrets
git_basic_authorization(ENV["MATCH_GIT_BASIC_AUTHORIZATION"])

app_identifier(["com.vyrlabs.app"])

# IMPORTANT: Do NOT configure api_key in Matchfile.
# The App Store Connect API env vars are provided by the workflow.
